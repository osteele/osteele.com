os:multiclone a doap:Project ;
    dc:title "Multiclone" ;
    doap:repository "https://github.com/osteele/multiclone" ;
    dc:description "Fast tool for cloning all forks of a repository or GitHub Classroom assignments." ;
    os:category "education", "educator-tools", "command-line-tool" ;
    schema:dateCreated "2017-10-13T17:49:43Z" ;
    schema:dateModified "2017-11-18T13:33:35Z" ;
    os:primaryLanguage "Python" ;
    os:isArchived false ;
    os:topics "github", "classroom", "education" ;
    os:exampleUsage """# Clone all forks of a repository
multiclone owner/repo

# Clone GitHub Classroom assignment repos
multiclone org/repo --classroom

# Clone into a specific directory
multiclone --dir path/to/dir owner/repo

# Preview commands without executing them
multiclone owner/repo --dry-run

# Clone repos listed in a file
multiclone repos.txt
""" .

